{
  "name": "badoo-styleguide",
  "version": "0.0.17",
  "description": "Badoo styleguide used to develop UI components for the Web and React Native",
  "author": "badoo",
  "license": "MIT",
  "bin": "./index.js",
  "scripts": {
    "deps": "yarn install --pure-lockfile",
    "build": "yarn start --config=styleguide.config.js --buildDir=./dist/",
    "start": "yarn deps && node ./index.js",
    "lint": "eslint . --fix --ext .js,.jsx",
    "prettier": "prettier --write '**/*.?(j|t)s?(x)'",
    "styleguide": "yarn start --config=styleguide.config.js"
  },
  "dependencies": {
    "@babel/core": "^7.4.0",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@babel/register": "^7.4.0",
    "babel-loader": "8.0.5",
    "classnames": "2.2.6",
    "css-loader": "2.1.0",
    "eslint": "^5.15.1",
    "eslint-plugin-react": "^7.12.4",
    "file-loader": "3.0.1",
    "happypack": "^5.0.1",
    "html-webpack-plugin": "3.2.0",
    "loader-utils": "^1.2.3",
    "metro-react-native-babel-preset": "^0.53.0",
    "node-sass": "4.11.0",
    "normalize.css": "8.0.1",
    "raw-loader": "1.0.0",
    "react-art": "^16.8.6",
    "react-docgen": "4.1.0",
    "react-hot-loader": "4.8.0",
    "react-native-svg-web": "^1.0.1",
    "react-native-web": "^0.10.1",
    "react-native-web-linear-gradient": "^1.0.5",
    "react-svg-loader": "2.1.0",
    "resolve-url-loader": "3.0.1",
    "sass-loader": "7.1.0",
    "style-loader": "0.23.1",
    "webpack": "4.29.6",
    "webpack-cli": "3.2.3",
    "webpack-dev-server": "3.2.1",
    "webpack-merge": "^4.2.1",
    "yargs": "13.2.1"
  },
  "peerDependencies": {
    "react": "^16.8.0",
    "react-dom": "16.8.4"
  },
  "devDependencies": {
    "prop-types": "^15.7.2",
    "react": "^16.8.4",
    "react-dom": "16.8.4"
  }
}
